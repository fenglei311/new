
.line{
	margin: 10px 0px 10px 0px;
	border: 1px solid lightgrey;
}
.center{
	display: flex;
	flex-direction: column;
}
.center_man{
	display: flex;
	flex-direction: row;
	margin-top: 25px;
	margin-left: 20px;
}
.center_man_img{
	width: 100px;
	height: 100px;
	border-radius: 100%;
	background: url(data:image/png;base64,) no-repeat center lightgrey;
}
.center_man_name{
	font-size: 24px;
	margin-left: 50px;
	margin-top: 30px;
}
.center_collect{
	width: 100%;
	height: 100px;
	line-height: 80px;
	/* border: 1px solid red; */
}
.center_history{
	width: 100%;
	height: 100px;
	line-height: 80px;
	/* border: 1px solid red; */
}
.center_settings{
	width: 100%;
	height: 70px;
	text-align: center;
	line-height: 70px;
	border: 1px solid grey;
}
.center_exit{
	width: 100%;
	height: 70px;
	margin-top: 20px;
	text-align: center;
	line-height: 70px;
	border: 1px solid grey;
	color: red;
}

